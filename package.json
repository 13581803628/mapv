{
  "name": "mapv",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deal": "VERSION=`node -e 'console.log(require(\"./package.json\").version)'` && cp build/mapv.js build/mapv.v${VERSION}.js",
    "predeal": "npm run build && uglifyjs build/mapv.js -c -m -o build/mapv.min.js",
    "build": "mkdir -p build && node -e 'process.stdout.write(\"export var version = \\\"\" + require(\"./package.json\").version + \"\\\";\\n\");' > build/version.js && rollup --format umd --name mapv -o build/mapv.js -- index.js ",
    "watch": "npm-watch",
    "test": "npm run watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "index.js"
      ],
      "extensions": "js,jsx",
      "ignore": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huiyan-fe/mapv.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huiyan-fe/mapv/issues"
  },
  "homepage": "https://github.com/huiyan-fe/mapv#readme",
  "devDependencies": {
    "rollup": "^0.25.4",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "npm-watch": "^0.1.1"
  }
}
